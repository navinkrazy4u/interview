<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>InterviewWS</servlet-name>
		<servlet-class>navin.anand.InterviewWSServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InterviewWS</servlet-name>
		<url-pattern>/interview</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Controller</servlet-name>
		<servlet-class>navin.anand.Controller</servlet-class>
	</servlet>
	<servlet>
        <servlet-name>guestbook</servlet-name>
        <servlet-class>navin.anand.GuestbookServlet</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>Controller</servlet-name>
		<url-pattern>/Controller</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>guestbook</servlet-name>
		<url-pattern>/guestbook</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>guestbook.jsp</welcome-file>

		<welcome-file>index.html</welcome-file>
		<welcome-file>interview.jsp</welcome-file>
		<welcome-file>dataquery.jsp</welcome-file>
		<welcome-file>dataload.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>
</web-app>